{
  "name": "vitta-companion",
  "private": true,
  "description": "l'application Vitta Companion permet la connexion entre le robot Niryo Ned2 et la plateforme Vittascience",
  "version": "1.0.1",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && ./builderSign.sh && npm run clean-extra",
    "clean-extra": "rm -rf build/mac-arm64",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "electron-is-dev": "^2.0.0",
    "electron-log": "^5.0.1",
    "electron-updater": "^6.1.7",
    "node-ssh": "^13.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-loader-spinner": "^6.1.1-0",
    "react-router-dom": "^6.18.0",
    "roslib": "^1.3.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "temp": "^0.9.4",
    "tempfile": "^5.0.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/ssh2": "^1.11.16",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.16",
    "electron": "^26.1.0",
    "electron-builder": "^24.6.4",
    "eslint": "^8.48.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.31",
    "rollup-plugin-visualizer": "^5.11.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-electron": "^0.14.0",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "main": "dist-electron/main.js",
  "build": {
    "appId": "com.electron.vitta-companion",
    "productName": "Vitta Companion",
    "copyright": "Copyright Â© 2023 Vittascience",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist-electron/**/*",
      "dist/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icons/favicon.ico"
    },
    "mac": {
      "target": "dmg",
      "category": "public.app-category.utilities",
      "icon": "assets/icons/favicon.icns"
    },
    "publish": [
      {
        "provider": "github",
        "repo": "vitta-companion",
        "owner": "Nixoals"
      }
    ]
  }
}
